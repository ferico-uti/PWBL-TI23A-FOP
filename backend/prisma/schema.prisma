generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// buat model (tb_user)
model tb_user {
  id       Int    @id @default(autoincrement())
  nama     String @db.VarChar(100)
  username String @db.VarChar(20)
  password String @db.Char(60)
}
